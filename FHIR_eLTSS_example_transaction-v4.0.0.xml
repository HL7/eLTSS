<!--
    Original : 5/22/2018
    Updated : 
    FHIR version : 3.4 (build 14355)
    Description : Sample XML instance based on mapping of Electronic Long Term Service and Support (eLTSS) consensus 
                  data elements mapped to the FHIR Care Plan resource. XML comments are used to indicate
                  the eLTSS consensus data element(s) being represented by FHIR data elements in this XML sample. For
                  example, the notation "[eLTSS Person Name]" indicates that the FHIR data element(s) following 
                  correspond to the eLTSS consensus data element "Person Name". 
                  NOTE 1: This sample has been generated as a FHIR bundle to illustrate the mapped FHIR resources in 
                  a single instance for convenience and in order to provide a sense of how the pieces fit together. 
                  However, the use of a bundle here does not indicate that implementers must use a bundle. A 
                  transaction bundle is being used here so that the sample can be easily added to a HAPI FHIR server.
                  NOTE 2: This sample includes displayable text in many of the resources. These text elements are not
                  documented as part of the FHIR mapping to the eLTSS consensus data elements, but are included here
                  to provide human readable representations of some of the key aspects of the service plan.
-->
<Bundle xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hl7.org/fhir ./FHIR%204.0.0/bundle.xsd">
	<type value="transaction"/>
	<!--  eLTSS Service Plan / FHIR Care Plan  -->
	<entry>
		<resource>
			<CarePlan>
				<id value="careplan1"/>
				<text>
					<status value="generated"/>
					<div xmlns="http://www.w3.org/1999/xhtml">
						<p>
							<b>Sally Person's Long Term Services and Supports (LTSS) Service Plan</b>
						</p>
						<p>
							<b>Plan Effective Date:</b> 2018-05-01 - 2018-12-31</p>
						<p>
							<b>Step or Action:</b> client action - manage diet</p>
						<p>
							<b>Step or Action:</b> client action - exercise / join a fitness club</p>
					</div>
				</text>
				<status value="active"/>
				<intent value="plan"/>
				<subject>
            		<reference value="Patient/patient1"/>
            	</subject>
				<!--  R4 change, R4 change, old version allowed context which could reference EpisodeOfCare 
                    this version doesn't have any link between the CarePlan and the EpisodeOfCare, so 
                    instance will need to be updated once we determine how we want to handle eLTSS Plan Monitor
                <context>
                    <reference value="EpisodeOfCare/episode1"/>
				</context>
                -->
				<!--  [eLTSS Plan Effective Date]  -->
				<period>
					<start value="2018-05-01"/>
					<end value="2018-12-31"/>
				</period>
				<author>
                    <reference value="Practitioner/planner1"/>
				</author>
				<addresses>
                    <reference value="Condition/need1"/>
				</addresses>
				<addresses>
                    <reference value="Condition/need2"/>
				</addresses>
				<addresses>
                    <reference value="Condition/need3"/>
				</addresses>
				<supportingInfo>
                    <reference value="Observation/strength1"/>
				</supportingInfo>
				<supportingInfo>
                    <reference value="Questionnaire/questionnaire1"/>
				</supportingInfo>
				<supportingInfo>
                    <reference value="Contract/contract1"/>
				</supportingInfo>
				<supportingInfo>
                    <reference value="RiskAssessment/risk1"/>
				</supportingInfo>
				<goal>
                    <reference value="Goal/goal1"/>
				</goal>
				<goal>
                    <reference value="Goal/goal2"/>
				</goal>
				<activity>
					<!--  [eLTSS Step or Action]  -->
					<detail>
						<goal>
                            <reference value="Goal/goal1"/>
						</goal>
						<status value="not-started"/>
						<description value="manage diet"/>
					</detail>
				</activity>
				<activity>
					<!--  [eLTSS Step or Action]  -->
					<detail>
						<goal>
                            <reference value="Goal/goal2"/>
						</goal>
						<status value="not-started"/>
						<description value="exercise / join a fitness club"/>
					</detail>
				</activity>
				<activity>
					<reference>
                        <reference value="ServiceRequest/service1"/>
					</reference>
				</activity>
				<activity>
					<reference>
                        <reference value="ServiceRequest/service2"/>
					</reference>
				</activity>
				<activity>
					<reference>
                        <reference value="ServiceRequest/service3"/>
					</reference>
				</activity>
			</CarePlan>
		</resource>
		<!-- Action to create this resource in a FHIR server -->
		<request> 
            <method value="POST"/> 
            <url value="CarePlan"/> 
        </request>
	</entry>
